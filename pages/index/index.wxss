.page_title_con{
    width: 750rpx;
    height: 37px;
    line-height: 37px;
    background: #fff;

    color: #333;
    position: fixed;
    left: 0;
    top:0;
    z-index: 20;
    text-align: center;
    padding-bottom:15rpx;
    overflow: hidden;
    font-weight: bold;

    transform: translateY(-100px);
    transition: transform 0.1s ease-in;
}
.page_title_con.on{
    transform: translateY(0);
}
.page_title{
    width:750rpx;
    text-align: center;
    height: 37px;
    line-height: 37px;
    overflow: hidden;
    position: absolute;
    left:0;
    font-size: 36rpx;
    color: #333;
    z-index: 10;
    font-weight: bold;
}
.search_con{


    width:686rpx;
    height: 68rpx;
    position: absolute;
    left: 32rpx;
    top: 188rpx;
    z-index:100;
    border-radius: 68rpx;
    display: flex;
    flex-direction: row;
    background: #A1A1A1;    
}
.search_con .search__img{
    display: block;
    width: 43rpx;
    height: 43rpx;
    margin-top: 12rpx;

    margin-left: 24rpx;
}
.search_con .search{
    display: block;
    height: 68rpx;
    width: 575rpx;
    line-height: 68rpx;
    font-size: 28rpx;
    color: #D9D9D9;
    padding-left: 15rpx;
}
.search_con .search ::placeholder {
   color: #D9D9D9;
}
.swiper_con_con{
  overflow: hidden;
  position: relative;
}
.swiper_con{
  height:800rpx;
}
.swiper_con .img{
    display:block;
    width:750rpx;
    height:800rpx;
    object-fit: cover;
}
.charts_con{
    width: 686rpx;
    overflow: hidden;
    margin:48rpx auto 80rpx;
    display: flex;
    flex-direction:row;
    justify-content:space-between;
}
.big_img{
    width:446rpx;
    height:352rpx;
    overflow: hidden;
}
.small_img{
    width:224rpx;
    height: 168rpx;
}
.big_img image,
.small_img image{
    display: block;
    width: 100%;   
    height: 100%;
    object-fit: cover;
}
.small_img_list{
    width:224rpx;
    display:flex;
    flex-direction: column;
    justify-content: space-between;
    height:352rpx;
}
.text1{
     display: block;
     margin:0 auto;
     width: 306rpx;
     height: 468rpx;
}

.vedio_list_con{
  width: 686rpx;
  height: 600rpx;
  padding:32rpx;
  margin: 80rpx 0;
  background: url("https://luockoo.oss-cn-shenzhen.aliyuncs.com/file/%E5%BE%AE%E4%BF%A1%E5%9B%BE%E7%89%87_20241108191743.png") no-repeat;
  background-size: cover;
}
.vedio_list_con .big_title{
  font-size: 32rpx;
  color: #fff;
  margin-top: 8rpx;
}
.vedio_list_con .big_msg{ 
  color:#C6A189 ;
  font-size: 36rpx;
  text-transform: uppercase;
  margin: 8rpx 0 40rpx;
}
.text2{
  display: block;
  width:272rpx;
  height: 152rpx;
  margin: 0 auto;
}
.vedio_list{
   overflow:hidden;
}
.vedio_list_scroll {
    width: 718rpx;
}
.vedio_list_scroll .vedio_list{
     width: 1510rpx;
     overflow: hidden;
     padding-right: 15rpx;
}
.vedio_item{
    width:484rpx;
    margin-left: 20rpx;
    float: left;
    position: relative;
}
.vedio_item:nth-child(1){
  margin-left: 0;
}
.vedio_item .img{
  display: block;
  width:484rpx;
  height: 324rpx;
}
.vedio_item .play_icon {
   display: block;
   width: 60rpx;
   height: 60rpx;
   position: absolute;
   left: 50%;
   margin-left:-30rpx;
   top:50%;
   margin-top:-30rpx;
}
.vedio_item .text_con{
    width: 100%;
    height: 48rpx;
    line-height: 48rpx;
    position: absolute;
    left: 0;
    bottom: 0;
    background: #000;
    color: #fff;
}

.vedio_item .text_con .txt{
    height: 48rpx;
    line-height: 48rpx;
    float: left;
    font-size:24rpx;
    margin-left: 20rpx;
}

.vedio_item .text_con .count{
     height: 48rpx;     
     line-height: 48rpx;
     float: right;
     margin-right: 20rpx;
}
.vedio_item .text_con .count image{
  float: left;
  display: block;
  width: 28rpx;
  height: 28rpx;
  margin-top:10rpx;
  margin-right: 10rpx;
}
.new_good_list{
    overflow: hidden;
    margin:60rpx 0;
    display: flex;
    flex-direction: row;
    padding:0 28rpx;
    flex-wrap: wrap;
    justify-content: space-between;
}
.more_btn{
     display: block;
     width: 136rpx;
     height: 50rpx;
     line-height: 50rpx;
     text-align: center;

     border:1px solid #000;
     font-size: 24rpx;
     margin:0 auto;
}

.hot_sell_con{
   margin: 128rpx 0 80rpx;
}

.hot_sell_con .title_con{
    margin: 0 32rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-end;
}
.hot_sell_con .title_con .title_img{
     display: block;
     width:256rpx;
     height: 154rpx;
}
.hot_sell_con .title_con .more_btn{
      margin: 0;
}

.hot_sell_con .good_list_scroll{
      width:718rpx;
      margin-top: 54rpx;
      margin-left: 32rpx;
}
.hot_sell_con .good_list_scroll .good_list{
      width:1500rpx;
      height: 580rpx;
}
.big_back{
      display: block;
      width: 750rpx;
      height: 300rpx;
}
.style_nav_list_con{
      overflow: hidden;
      margin: 20rpx 0;
      display: flex;
      flex-direction: row;
      /* border: 1px solid brown; */
}

.style_nav_list{
      /* height: 95rpx; */
      overflow: hidden;
}
.style_nav{
      max-width:1000rpx;
      height: 88rpx;
      display: flex;
      flex-direction: row;
}
.style_nav_list .item{
     width:60rpx;
